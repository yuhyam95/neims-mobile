{
  "expo": {
    "name": "NEIMS",
    "slug": "neims-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/nema-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/nema-logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.sparks95.neimsmobile"
    },
    "web": {
      "favicon": "./assets/nema-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "ab94d0cd-5eb5-436f-bf8b-f1de9d2591fb"
      }
    }
  }
}
